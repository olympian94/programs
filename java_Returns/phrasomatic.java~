public class phrasomatic
{
	public static void main(String[] args)
	{
		String[] wordlist1 = {"amazing","new","24/7","delivering","discreet","amazing","startling","tremendous","game-changing"};
		String[] wordlist2 = {"solution","program","plan","company","office","room","place","car","phone"};
		int len1= wordlist1.length;
		int len2= wordlist2.length;
		int rand1 = (int)(Math.random()*len1);
		int rand2 = (int)(Math.random()*len2);
		String phrase = wordlist1[rand1] + " " + wordlist2[rand2];
		System.out.println("Hy "+phrase);
	}
}
